{
    "name": "brycematheson/laravel_ab_test",
    "description": "Laravel 10 AB testing package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apurbajnu\\Abtest\\": "src/"
        }
    },

    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.9.*",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "apurbajnu\\Abtest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apurbajnu\\Abtest\\AbtestServiceProvider"
            ],
            "aliases": {
                "Abtest": "Apurbajnu\\Abtest\\AbtestFacade"
            }
        }
    }
}
